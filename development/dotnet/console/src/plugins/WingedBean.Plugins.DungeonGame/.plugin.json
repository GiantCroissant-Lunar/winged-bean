{
  "id": "wingedbean.plugins.dungeongame",
  "name": "Dungeon Game Logic",
  "version": "1.0.0",
  "description": "Dungeon crawler game logic plugin with ECS and reactive patterns",
  "author": "WingedBean",
  "entryPoint": { "dotnet": "WingedBean.Plugins.DungeonGame.dll" },
  "exports": {
    "services": [
      {
        "interface": "Plate.CrossMilo.Contracts.Game.Dungeon.IService",
        "implementation": "WingedBean.Plugins.DungeonGame.DungeonGameService",
        "lifecycle": "singleton"
      },
      {
        "interface": "Plate.CrossMilo.Contracts.Game.Render.IService",
        "implementation": "WingedBean.Plugins.DungeonGame.Services.RenderServiceProvider",
        "lifecycle": "singleton"
      }
    ]
  },
  "capabilities": [ "game-logic" ],
  "loadStrategy": "eager"
}
