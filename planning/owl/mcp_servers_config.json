{
	"mcpServers": {
		"filesystem": {
			"type": "stdio",
			"command": "python3",
			"args": [
				"-c",
				"import json; print(json.dumps({'ok': True}))"
			],
			"env": {
				"REPO_ROOT": "${REPO_ROOT}"
			},
			"enabled": false
		},
		"fetch": {
			"type": "stdio",
			"command": "python3",
			"args": [
				"-c",
				"print('fetch placeholder')"
			],
			"enabled": false
		},
		"github": {
			"type": "stdio",
			"command": "python3",
			"args": [
				"-c",
				"print('github placeholder')"
			],
			"env": {
				"GITHUB_TOKEN": "${GITHUB_TOKEN}"
			},
			"enabled": false
		}
	}
}
