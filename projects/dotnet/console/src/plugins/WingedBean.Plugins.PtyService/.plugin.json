{
  "id": "winged-bean.pty-service",
  "name": "PTY Service",
  "version": "1.0.0",
  "description": "Provides PTY (Pseudo Terminal) functionality using Node.js node-pty",
  "author": "WingedBean Team",
  "entryPoint": { "dotnet": "WingedBean.Plugins.PtyService.dll" },
  "dependencies": {},
  "exports": {
    "services": [
      {
        "interface": "WingedBean.Contracts.IPtyService",
        "implementation": "WingedBean.Plugins.PtyService.NodePtyService",
        "lifecycle": "singleton"
      }
    ]
  },
  "capabilities": [
    "pty-spawning",
    "process-management",
    "terminal-io"
  ],
  "tags": ["pty", "terminal", "nodejs", "process"],
  "metadata": {
    "requiresNodejs": true,
    "nodeVersion": ">=18.0.0",
    "nodePtyPath": "../../../nodejs/pty-service"
  }
}
